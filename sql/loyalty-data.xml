<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Security//EN"
    "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
  <import-items>
    <!-- 2 items of type concert -->

    <add-item item-descriptor="user" id="3000">
      <set-property name="login"><![CDATA[ksy]]></set-property>
      <set-property name="name"><![CDATA[first]]></set-property>
      <set-property name="description"><![CDATA[no descr]]></set-property>
      <set-property name="publish"><![CDATA[true]]></set-property>
      <set-property name="songs" value="200051"/>
    </add-item>

    <add-item item-descriptor="playlist" id="3001" >
      <set-property name="login"><![CDATA[second]]></set-property>
      <set-property name="description"><![CDATA[no descr]]></set-property>
      <set-property name="publish"><![CDATA[true]]></set-property>
      <set-property name="songs" value="200052"/>
    </add-item>

    <add-item item-descriptor="user" id="3002">
      <set-property name="playlists"><![CDATA[100001,100002]]></set-property>
    </add-item>

  </import-items>

</gsa-template>
